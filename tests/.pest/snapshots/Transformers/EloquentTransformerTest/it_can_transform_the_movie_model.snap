interface Movie {
    id: number;
    title: string;
    plot?: string;
    rating?: number;
    metadata?: any[];
    box_office?: number;
    runtime?: number;
    release_date?: string;
    premiered_at?: string;
    is_released: boolean;
    director_id?: number;
    created_at?: string;
    updated_at?: string;
    director?: Person;
    cast: Person & { pivot: Casting };
    awards: any[];
}